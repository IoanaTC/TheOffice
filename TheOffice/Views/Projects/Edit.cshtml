@model TheOffice.Models.Project

<h2 class="text-center">Edit a Project!</h2>

<br />

<div class="container">

    <form enctype="multipart/form-data" asp-controller="Projects" asp-action="Edit">

        <partial name="ProjectForm" model=@Model />
        <a href="/Projects/EditMembers/@Model.Id" class="btn btn-outline-warning m-2">Edit Members</a>

        <button class="btn btn-success" type="submit"> Edit </button>
    </form>
</div>
